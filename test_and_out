bash-3.2$ unset SHLVL SHELL
bash-3.2$ echo $SHLVL $SHELL

bash-3.2$ bash
bash-3.2$ echo $SHLVL $SHELL
1 /bin/zsh

bash-3.2$ cat < 1
bash: 1: No such file or directory

bash-3.2$ chmod -r 1
bash-3.2$ cat < 1
